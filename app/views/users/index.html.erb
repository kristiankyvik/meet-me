<table class="table">
<tbody>
  <tr>
    <th>Id</th>
    <th>User Name</th>
    <th>Name</th>
    <th>Nationality</th>
    <th>Visits</th>
    <th>Actions</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td> 
    <td><%= link_to user.user_name, user_path(user.id) %></td> 
    <td> <%=  user.name %></td>
    <td> <%=  user.nationality %></td>
    <td> <%=  link_to user.visits.count, user_visits_path(user.id)%></td>
    <td> <%=  link_to "delete", user_path(user.id),
            method: 'delete' , confirm: "Are you sure?"%>
            <%=  link_to "edit", edit_user_path(user.id),
            method: 'get' %>
    </td>
  </tr>
<% end %>

 </tbody>
</table>
<div class="col-md-10 rd">
  <h1>User profile @user.user_name</h1>
  <h2><%= @user.name %><%=  @user.nationality %></h2>
</div>

<table class="table">
<tr>
    <th>Id</th>
    <th>Location</th>
    <th>From</th>
    <th>To</th>
  </tr>
  <tbody>
    <% @visits.each do |visit| %>
    <tr>
      <td>
        <%= visit.id %> 
      </td> 
      <td>
        <%= visit.location_id %>
      </td>
      <td>
        <%= visit.from_date %> 
      </td>
      <td>
        <%= visit.to_date %> 
      </td> 
    </tr>
    <% end %>
  </tbody>
</table>